{
  "name": "front_end",
  "version": "1.0.0",
  "description": "front end for 401 desktop app ",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.p.id",
    "productName": "Poseidon", 
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/CSCI401TEAM/XXXFront-end.git"
  },
  "author": "Sid Qian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CSCI401TEAM/XXXFront-end/issues"
  },
  "homepage": "https://github.com/CSCI401TEAM/XXXFront-end#readme",
  "dependencies": {
    "cesium": "^1.113.0",
    "golden-layout": "^1.5.9",
    "satellite.js": "^5.0.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1"
  }
}
